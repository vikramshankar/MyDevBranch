/*C/C++ program to allocate memory for a 2d array*/

#include <iostream>
using namespace std;

int main()
{
  int** a = new int*[10];   // create a double pointer to point to an array of int
  cout<<a[0];
  for(int i = 0;i<10;i++)
    a[i] = new int[10];     // allocate memory to the columns
  a[3][3] = 10;             // assign data
  cout<<a[3][3];            
}
